<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
      <li Class="ModCheck.isModLoaded">
        <modName>[RF] Fertile Fields [b18]</modName>
        <yourMod>Extended Woodworking</yourMod>
      </li>
      <li Class="ModCheck.loadOrder">
        <modName>[RF] Fertile Fields [b18]</modName>
        <yourMod>Extended Woodworking</yourMod>
        <errorOnFail>true</errorOnFail>
      </li>
      <li Class="PatchOperationTest">
        <xpath>Defs/*[defName="CompostBin"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>Defs/*[defName="CompostBin"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>Defs/*[defName="CompostBin" and (stuffCategories)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="CompostBin"]/stuffCategories</xpath>
          <value>
            <li>Woody</li>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>Defs/*[defName="CompostBin"]</xpath>
          <value>
            <stuffCategories>
              <li>Woody</li>
            </stuffCategories>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="CompostBin" and not(costStuffCount)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="CompostBin"]</xpath>
          <value>
            <costStuffCount>2</costStuffCount>
          </value>
        </match>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="CompostBin"]/constructEffect</xpath>
        <match Class="PatchOperationRemove">
          <xpath>Defs/*[defName="CompostBin"]/constructEffect</xpath>
        </match>
      </li>
      <li Class="PatchOperationTest">
        <xpath>Defs/*[defName="CompostBarrel"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>Defs/*[defName="CompostBarrel"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>Defs/*[defName="CompostBarrel" and (stuffCategories)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="CompostBarrel"]/stuffCategories</xpath>
          <value>
            <li>Woody</li>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>Defs/*[defName="CompostBarrel"]</xpath>
          <value>
            <stuffCategories>
              <li>Woody</li>
            </stuffCategories>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="CompostBarrel" and not(costStuffCount)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="CompostBarrel"]</xpath>
          <value>
            <costStuffCount>10</costStuffCount>
          </value>
        </match>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="CompostBarrel"]/constructEffect</xpath>
        <match Class="PatchOperationRemove">
          <xpath>Defs/*[defName="CompostBarrel"]/constructEffect</xpath>
        </match>
      </li>
    </operations>
  </Operation>
</Patch>