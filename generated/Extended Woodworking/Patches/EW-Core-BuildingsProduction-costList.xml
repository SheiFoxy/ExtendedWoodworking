<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
      <li Class="ModCheck.isModLoaded">
        <modName>Core</modName>
        <yourMod>Extended Woodworking</yourMod>
      </li>
      <li Class="ModCheck.loadOrder">
        <modName>Core</modName>
        <yourMod>Extended Woodworking</yourMod>
        <errorOnFail>true</errorOnFail>
      </li>
      <li Class="PatchOperationTest">
        <xpath>Defs/*[defName="Brewery"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>Defs/*[defName="Brewery"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>Defs/*[defName="Brewery" and (stuffCategories)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Brewery"]/stuffCategories</xpath>
          <value>
            <li>Woody</li>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Brewery"]</xpath>
          <value>
            <stuffCategories>
              <li>Woody</li>
            </stuffCategories>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="Brewery" and not(costStuffCount)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Brewery"]</xpath>
          <value>
            <costStuffCount>120</costStuffCount>
          </value>
        </match>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="Brewery"]/constructEffect</xpath>
        <match Class="PatchOperationRemove">
          <xpath>Defs/*[defName="Brewery"]/constructEffect</xpath>
        </match>
      </li>
      <li Class="PatchOperationTest">
        <xpath>Defs/*[defName="FermentingBarrel"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>Defs/*[defName="FermentingBarrel"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>Defs/*[defName="FermentingBarrel" and (stuffCategories)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="FermentingBarrel"]/stuffCategories</xpath>
          <value>
            <li>Woody</li>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>Defs/*[defName="FermentingBarrel"]</xpath>
          <value>
            <stuffCategories>
              <li>Woody</li>
            </stuffCategories>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="FermentingBarrel" and not(costStuffCount)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="FermentingBarrel"]</xpath>
          <value>
            <costStuffCount>30</costStuffCount>
          </value>
        </match>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="FermentingBarrel"]/constructEffect</xpath>
        <match Class="PatchOperationRemove">
          <xpath>Defs/*[defName="FermentingBarrel"]/constructEffect</xpath>
        </match>
      </li>
    </operations>
  </Operation>
</Patch>