<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
      <li Class="ModCheck.isModLoaded">
        <modName>[T] ExpandedCloth</modName>
        <yourMod>Extended Woodworking</yourMod>
      </li>
      <li Class="ModCheck.loadOrder">
        <modName>[T] ExpandedCloth</modName>
        <yourMod>Extended Woodworking</yourMod>
        <errorOnFail>true</errorOnFail>
      </li>
      <li Class="PatchOperationTest">
        <xpath>Defs/*[defName="BenchTextile"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>Defs/*[defName="BenchTextile"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>Defs/*[defName="BenchTextile" and (stuffCategories)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="BenchTextile"]/stuffCategories</xpath>
          <value>
            <li>Woody</li>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>Defs/*[defName="BenchTextile"]</xpath>
          <value>
            <stuffCategories>
              <li>Woody</li>
            </stuffCategories>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="BenchTextile" and not(costStuffCount)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="BenchTextile"]</xpath>
          <value>
            <costStuffCount>20</costStuffCount>
          </value>
        </match>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="BenchTextile"]/constructEffect</xpath>
        <match Class="PatchOperationRemove">
          <xpath>Defs/*[defName="BenchTextile"]/constructEffect</xpath>
        </match>
      </li>
    </operations>
  </Operation>
</Patch>