<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
      <li Class="ModCheck.isModLoaded">
        <modName>Core</modName>
        <yourMod>Extended Woodworking</yourMod>
      </li>
      <li Class="ModCheck.loadOrder">
        <modName>Core</modName>
        <yourMod>Extended Woodworking</yourMod>
        <errorOnFail>true</errorOnFail>
      </li>
      <li Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[defName="PlantTreeDrago"]/plant</xpath>
        <value>
          <harvestedThingDef>WoodLog_Drago</harvestedThingDef>
        </value>
      </li>
    </operations>
  </Operation>
</Patch>