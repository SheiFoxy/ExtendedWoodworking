<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
      <li Class="ModCheck.isModLoaded">
        <modName>Core</modName>
        <yourMod>Extended Woodworking</yourMod>
      </li>
      <li Class="ModCheck.loadOrder">
        <modName>Core</modName>
        <yourMod>Extended Woodworking</yourMod>
        <errorOnFail>true</errorOnFail>
      </li>
      <li Class="PatchOperationTest">
        <xpath>Defs/*[defName="Bow_Short"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>Defs/*[defName="Bow_Short"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>Defs/*[defName="Bow_Short" and (stuffCategories)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Bow_Short"]/stuffCategories</xpath>
          <value>
            <li>Woody</li>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Bow_Short"]</xpath>
          <value>
            <stuffCategories>
              <li>Woody</li>
            </stuffCategories>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="Bow_Short" and not(costStuffCount)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Bow_Short"]</xpath>
          <value>
            <costStuffCount>30</costStuffCount>
          </value>
        </match>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="Bow_Short"]/constructEffect</xpath>
        <match Class="PatchOperationRemove">
          <xpath>Defs/*[defName="Bow_Short"]/constructEffect</xpath>
        </match>
      </li>
      <li Class="PatchOperationTest">
        <xpath>Defs/*[defName="Pila"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>Defs/*[defName="Pila"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>Defs/*[defName="Pila" and (stuffCategories)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Pila"]/stuffCategories</xpath>
          <value>
            <li>Woody</li>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Pila"]</xpath>
          <value>
            <stuffCategories>
              <li>Woody</li>
            </stuffCategories>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="Pila" and not(costStuffCount)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Pila"]</xpath>
          <value>
            <costStuffCount>70</costStuffCount>
          </value>
        </match>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="Pila"]/constructEffect</xpath>
        <match Class="PatchOperationRemove">
          <xpath>Defs/*[defName="Pila"]/constructEffect</xpath>
        </match>
      </li>
      <li Class="PatchOperationTest">
        <xpath>Defs/*[defName="Bow_Recurve"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>Defs/*[defName="Bow_Recurve"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>Defs/*[defName="Bow_Recurve" and (stuffCategories)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Bow_Recurve"]/stuffCategories</xpath>
          <value>
            <li>Woody</li>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Bow_Recurve"]</xpath>
          <value>
            <stuffCategories>
              <li>Woody</li>
            </stuffCategories>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="Bow_Recurve" and not(costStuffCount)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Bow_Recurve"]</xpath>
          <value>
            <costStuffCount>40</costStuffCount>
          </value>
        </match>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="Bow_Recurve"]/constructEffect</xpath>
        <match Class="PatchOperationRemove">
          <xpath>Defs/*[defName="Bow_Recurve"]/constructEffect</xpath>
        </match>
      </li>
      <li Class="PatchOperationTest">
        <xpath>Defs/*[defName="Bow_Great"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>Defs/*[defName="Bow_Great"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>Defs/*[defName="Bow_Great" and (stuffCategories)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Bow_Great"]/stuffCategories</xpath>
          <value>
            <li>Woody</li>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Bow_Great"]</xpath>
          <value>
            <stuffCategories>
              <li>Woody</li>
            </stuffCategories>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="Bow_Great" and not(costStuffCount)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>Defs/*[defName="Bow_Great"]</xpath>
          <value>
            <costStuffCount>50</costStuffCount>
          </value>
        </match>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>Defs/*[defName="Bow_Great"]/constructEffect</xpath>
        <match Class="PatchOperationRemove">
          <xpath>Defs/*[defName="Bow_Great"]/constructEffect</xpath>
        </match>
      </li>
    </operations>
  </Operation>
</Patch>