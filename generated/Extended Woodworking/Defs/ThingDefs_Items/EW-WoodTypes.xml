<Defs>
  <ThingDef ParentName="ResourceVerbBase">
    <defName>WoodLog</defName>
    <label>generic wood</label>
    <description>The most generic piece of wood you've ever seen. So generic you can't even tell what kind of wood it is.</description>
    <graphicData>
      <texPath>Things/Item/Resource/WoodLog</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <equippedAngleOffset>-20</equippedAngleOffset>
    <soundInteract>Wood_Drop</soundInteract>
    <soundDrop>Wood_Drop</soundDrop>
    <statBases>
      <MarketValue>1.2</MarketValue>
      <MaxHitPoints>150</MaxHitPoints>
      <Mass>0.4</Mass>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>1</DeteriorationRate>
      <SharpDamageMultiplier>0.3</SharpDamageMultiplier>
      <BluntDamageMultiplier>0.8</BluntDamageMultiplier>
