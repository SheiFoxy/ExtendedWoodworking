<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationSequence">
    <operations>
      <success>Always</success>
      <li Class="ModCheck.isModLoaded">
        <modName>Core</modName>
        <yourMod>Extended Woodworking</yourMod>
      </li>
      <li Class="ModCheck.loadOrder">
        <modName>Core</modName>
        <yourMod>Extended Woodworking</yourMod>
        <errorOnFail>true</errorOnFail>
      </li>
      <li Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef[defName="PlantTreeTeak"]/plant</xpath>
        <value>
          <harvestedThingDef>WoodLog_Teak</harvestedThingDef>
        </value>
      </li>
      <li Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef[defName="PlantTreeCecropia"]/plant</xpath>
        <value>
          <harvestedThingDef>WoodLog_Cecropia</harvestedThingDef>
        </value>
      </li>
      <li Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef[defName="PlantTreePalm"]/plant</xpath>
        <value>
          <harvestedThingDef>WoodLog_Palm</harvestedThingDef>
        </value>
      </li>
    </operations>
  </Operation>
</Patch>